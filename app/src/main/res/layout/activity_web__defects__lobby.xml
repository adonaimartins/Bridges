<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Web_Defects_Lobby">


    <LinearLayout
        android:id="@+id/statusContainer_p46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/status1_p46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            android:text="Deck1" />

        <TextView
            android:id="@+id/status2_p46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            android:text="Girder1" />

    </LinearLayout>

    <TextView
        android:id="@+id/title_web_p46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="168dp"
        android:text="@string/title_web_defects"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addButton_p46"
        android:layout_width="90dp"
        android:layout_height="43dp"
        android:layout_marginTop="80dp"
        android:layout_weight="1"
        android:onClick="addDefect"
        android:text="@string/buttonAddText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.856"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_web_p46" />

    <TextView
        android:id="@+id/tableNameDefects_p46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tableNameDefects"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addButton_p46" />

    <TableLayout
        android:id="@+id/tableLayout_p46"
        android:layout_width="match_parent"
        android:layout_height="129dp"
        android:padding="10dp"
        android:shrinkColumns="*"
        android:stretchColumns="*"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableNameDefects_p46"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/idRowTitle_p46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_span="1"
                android:layout_weight="1"
                android:gravity="center"
                android:text="ID" />

            <TextView
                android:id="@+id/typeRowTitle_p46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_span="1"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Type of Error" />

            <TextView
                android:id="@+id/empty_p46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_span="1"
                android:layout_weight="1"
                android:text="" />

            <TextView
                android:id="@+id/empty2_p46"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/idRow1_p46"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"/>

            <TextView
                android:id="@+id/errorTypeRow1_p46"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"/>


            <Button
                android:id="@+id/row_editDelete_p46"
                android:layout_width="35dp"
                android:layout_height="40dp"
                android:layout_marginRight="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:padding="0sp"
                android:text="@string/table_button_del"
                android:textSize="8dp" />
            <Button
                android:id="@+id/row_editDefect_p46"
                android:layout_width="35dp"
                android:layout_height="40dp"
                android:layout_marginRight="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:padding="0sp"
                android:text="@string/table_button_edit"
                android:textSize="8dp" />
        </TableRow>

    </TableLayout>

    <CheckBox
        android:id="@+id/completedButton_p46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="172dp"
        android:text="@string/checkboxCompletedText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout_p46"
        app:layout_constraintVertical_bias="0.396" />

    <Button
        android:id="@+id/submitButton_p46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:onClick="submit"
        android:text="@string/buttonBubmitText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/completedButton_p46"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout_p46"
        app:layout_constraintVertical_bias="0.996" />


</androidx.constraintlayout.widget.ConstraintLayout>