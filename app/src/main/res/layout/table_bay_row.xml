<?xml version="1.0" encoding="utf-8"?>
<TableRow
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/girderId_p28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/girderName_p28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="15dp" />
    <TextView
        android:id="@+id/girderBaysQty_p28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="15dp" />

    <CheckBox
        android:id="@+id/girderDoneCheck_p28"
        android:layout_width="5dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:editable="false" />

    <Button
        android:id="@+id/table_button_edit_p28"
        android:layout_width="25dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="0sp"
        android:layout_marginLeft="10dp"
        android:text="@string/table_button_edit"

        android:textSize="10dp" />

    <Button
        android:id="@+id/table_button_del_p28"
        android:layout_width="25dp"
        android:layout_height="40dp"
        android:layout_column="5"
        android:layout_marginLeft="20dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="0sp"
        android:text="@string/table_button_del"
        android:textSize="10dp" />



</TableRow>